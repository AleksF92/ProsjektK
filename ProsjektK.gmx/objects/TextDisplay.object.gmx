        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Handle events
if (enabled) {
    var btnClick = false;
    if (pages &gt; 1) {
        var w = sprite_get_width(spr_Page_Back);
        var h = sprite_get_height(spr_Page_Back);
    
        //Back button
        backState = 0;
        var bx = x1 - w;
        var by = y1;
        if (mouse_x &gt;= bx &amp;&amp; mouse_x &lt;= bx + w)
        &amp;&amp; (mouse_y &gt;= by &amp;&amp; mouse_y &lt;= by + h) {
            backState = 1;
            btnClick = true;
            if (mouse_check_button_released(mb_left)) {
                if (page &gt; 0) { 
                    page--;
                    textdisplay_page(page);
                }
            }
        }
        if (page &lt;= 0) { backState = -1; }
    
        //Next button
        nextState = 0;
        var nx = x2 + 1;
        var ny = y1;
        if (mouse_x &gt;= nx &amp;&amp; mouse_x &lt;= nx + w)
        &amp;&amp; (mouse_y &gt;= ny &amp;&amp; mouse_y &lt;= ny + h) {
            nextState = 1;
            btnClick = true;
            if (mouse_check_button_released(mb_left)) {
                if (page + 1 &lt; pages) { 
                    page++;
                    textdisplay_page(page);
                }
            }
        }
        if (page + 1 &gt;= pages) { nextState = -1; }
    }
    
    if (!btnClick) {
        if (mouse_check_button_released(mb_left)) {
            if (mouse_x &lt; x1 || mouse_x &gt; x2 || mouse_y &lt; y1 || mouse_y &gt; y2) {
                //Outside window
                textdisplay_hide();
            }
        }
    }
    
    if (keyboard_check_pressed(vk_escape)) {
        textdisplay_hide();
    }
}
</string>
